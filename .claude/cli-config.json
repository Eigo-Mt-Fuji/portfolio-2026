{
  "version": 1,
  "editor": {
    "vimMode": false
  },
  "hasChangedDefaultModel": false,
  "permissions": {
    "allow": [
      "Shell(ls)",
      "Shell()",
      "Read(**)"
    ],
    "deny": [
      "Shell(rm -rf *)",
      "Shell(git commit)",
      "Shell(git push)",
      "Read(src/envs/*)"
    ]
  },
  "network": {
    "useHttp1ForAgent": false
  }
}
